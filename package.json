{
  "name": "roles-privileges-payload-plugin",
  "version": "1.2.2",
  "description": "Automatic role-based access control (RBAC) plugin for Payload CMS that generates granular CRUD privileges for all collections with beautiful UI and zero configuration",
  "keywords": [
    "payload",
    "payload-cms",
    "payload-plugin",
    "payloadcms",
    "roles",
    "privileges",
    "rbac",
    "access-control",
    "permissions",
    "authorization",
    "auth",
    "user-roles",
    "role-based",
    "crud",
    "admin",
    "cms",
    "content-management"
  ],
  "author": "Hassine",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Hammoudahassine/roles-privileges-payload-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/Hammoudahassine/roles-privileges-payload-plugin/issues"
  },
  "homepage": "https://github.com/Hammoudahassine/roles-privileges-payload-plugin#readme",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./client": {
      "import": "./dist/exports/client.js",
      "types": "./dist/exports/client.d.ts",
      "default": "./dist/exports/client.js"
    },
    "./utilities": {
      "import": "./dist/exports/utilities.js",
      "types": "./dist/exports/utilities.d.ts",
      "default": "./dist/exports/utilities.js"
    },
    "./types": {
      "import": "./dist/exports/types.js",
      "types": "./dist/exports/types.d.ts",
      "default": "./dist/exports/types.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run copyfiles && npm run build:types && npm run build:swc",
    "build:swc": "swc ./src -d ./dist --config-file .swcrc --strip-leading-paths",
    "build:types": "tsc --outDir dist --rootDir ./src",
    "clean": "rimraf {dist,*.tsbuildinfo}",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png,json}\" dist/",
    "lint": "eslint",
    "lint:fix": "eslint ./src --fix"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@payloadcms/eslint-config": "3.9.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@swc/cli": "0.6.0",
    "@types/node": "^22.5.4",
    "copyfiles": "2.4.1",
    "eslint": "^9.23.0",
    "prettier": "^3.4.2",
    "rimraf": "3.0.2",
    "semantic-release": "^24.2.0",
    "sort-package-json": "^2.10.0",
    "typescript": "5.7.3",
    "payload": "3.37.0",
    "@payloadcms/ui": "3.37.0",
    "react": "19.2.3"
  },
  "peerDependencies": {
    "payload": "^3.69.0"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "./client": {
        "import": "./dist/exports/client.js",
        "types": "./dist/exports/client.d.ts",
        "default": "./dist/exports/client.js"
      },
      "./utilities": {
        "import": "./dist/exports/utilities.js",
        "types": "./dist/exports/utilities.d.ts",
        "default": "./dist/exports/utilities.js"
      },
      "./types": {
        "import": "./dist/exports/types.js",
        "types": "./dist/exports/types.d.ts",
        "default": "./dist/exports/types.js"
      }
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  },
  "registry": "https://registry.npmjs.org/"
}
